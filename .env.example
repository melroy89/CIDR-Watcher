# InfluxDB connection
INFLUX_ADDR="http://localhost:8086"
# InfluxDB database name
INFLUX_DB="telegraf"
# InfluxDB measurement name
INFLUX_MEASUREMENT="nginx_access_log"
# Optional username and password for InfluxDB
#INFLUX_USER=""
#INFLUX_PASS=""

# MySQL DSN. Example value below uses go-sql-driver/mysql format:
#   user:pass@tcp(host:port)/audit?parseTime=true
# MySQL DSN. Example value below uses go-sql-driver/mysql format with unix socket:
#   user:pass@unix(/var/run/mysqld/mysqld.sock)/audit?parseTime=true
MYSQL_DSN="root:password@unix(/var/run/mysqld/mysqld.sock)/audit?parseTime=true"

# Watch list file path - one CIDR per line
# Default: ./watch_list.txt
#WATCH_LIST_FILE="./watch_list.txt"

# Polling and reload intervals
#POLL_INTERVAL_SECONDS=30        # default poll interval in seconds
#WATCHLIST_RELOAD_SECONDS=1500   # default reload interval in seconds
#INFLUX_QUERY_LIMIT=5000         # maximum rows to query from InfluxDB per poll

# Mail/alerting configuration (optional)
#MAIL_FROM_NAME="CIDR Watcher"
#MAIL_FROM_ADDR="no-reply@example.com"
#MAIL_TO="alerts@example.com"
#ALERT_HIT_THRESHOLD=10          # number of hits before a mail notification is sent
